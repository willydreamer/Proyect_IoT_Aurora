<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondosupervisor">

    <TextView
        android:id="@+id/textView15"
        android:layout_width="86dp"
        android:layout_height="57dp"
        android:text="Apellido"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="164dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="111dp"
        android:layout_height="23dp"
        android:text="Nombre"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/button13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.132"
        app:layout_constraintStart_toStartOf="@+id/button13"
        app:layout_constraintTop_toTopOf="@+id/button13" />

    <Button
        android:id="@+id/button10"
        android:layout_width="114dp"
        android:layout_height="32dp"
        android:background="@drawable/boton_redondeado"
        android:backgroundTint="#96E6A7"
        android:text="Estado: activo"
        android:textSize="8sp"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="233dp" />

    <Button
        android:id="@+id/button12"
        android:layout_width="105dp"
        android:layout_height="30dp"
        android:background="@drawable/boton_redondeado"
        android:backgroundTint="#71B680"
        android:text="Subir foto"
        android:textSize="10sp"
        tools:layout_editor_absoluteX="273dp"
        tools:layout_editor_absoluteY="233dp" />

    <Button
        android:id="@+id/button13"
        android:layout_width="345dp"
        android:layout_height="55dp"
        android:background="@drawable/boton_redondeado"
        android:clickable="false"
        android:contextClickable="false"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="285dp" />

    <Button
        android:id="@+id/button14"
        android:layout_width="346dp"
        android:layout_height="49dp"
        android:background="@drawable/boton_redondeado"
        android:clickable="false"
        android:contextClickable="false"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="365dp" />

    <Button
        android:id="@+id/button15"
        android:layout_width="342dp"
        android:layout_height="54dp"
        android:background="@drawable/boton_redondeado"
        android:clickable="false"
        android:contextClickable="false"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="443dp" />

    <Button
        android:id="@+id/button16"
        android:layout_width="345dp"
        android:layout_height="53dp"
        android:background="@drawable/boton_redondeado"
        android:clickable="false"
        android:contextClickable="false"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="518dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:drawable/btn_radio"
        app:circularflow_defaultRadius="15sp"
        app:circularflow_radiusInDP="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.154"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/button17"
        android:layout_width="343dp"
        android:layout_height="52dp"
        android:background="@drawable/boton_redondeado"
        android:clickable="false"
        android:contextClickable="false"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="592dp" />

</androidx.constraintlayout.widget.ConstraintLayout>